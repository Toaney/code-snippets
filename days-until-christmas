var today = new Date();
var thisYear = today.getFullYear();
var christmas = new Date(thisYear, 11, 25)
console.log(today);
console.log(christmas);

if (today.getMonth >= 11 && today.getDay() > 25){
	christmas.setYear = (christmas.getFullYear() + 1)
}

var oneDay = 1000 * 60 * 60 * 24;

console.log(Math.ceil((christmas.getTime() - today.getTime())/oneDay) + " days until Christmas!");
